/* 通用样式 */
* {
	margin: 0;
	padding: 0;
	font-family: "宋体"；
}	
.w {
	width:1005px;
	margin: 0 auto;
}
a {
	text-decoration:none;
	color: #666;
	font-size: 12px;
}
h2 {
	font-weight: normal;
}
/* banner部分 */
.banner {
	width: 1440px;
	height: 397px;
	background:url(../images/banner.png);
}
.line {
	width: 100%;
	height: 6px;
	background-color: #015caf;
}
/* 导航部分 */
.nav {
	height: 417px;
}	
.nav-t {
	height: 54px;
	margin-bottom: 40px;
}
.subnav {
	width: 196px;
	float: left;
	text-align: center;
	line-height: 54px;
	margin-right: 6px;
	border-radius: 0 0 10px 10px; 
}
.subnav a {
	color: #fff;
}
.d1 {
	background-color: #0784f0;
}
.d2 {
	background-color: #c7243f;
}
.d3 {
	background-color: #6b40c5;
}
.d4 {
	background-color: #63ab2e;
}
.d5 {
	background-color: #f87e03;
	margin: 0;
}
.nav-b {
	height: 323px;
}
.schedule {
	float: left;
	width: 570px;
	border: 4px solid #eaeaea;
	height: 315px;
}
.schedule h4 {
	color: #474747;
	font-weight: normal;
	margin: 13px 0 10px 10px;
}
.schedule table{
	width: 545px;
	height: 252px;
	border-collapse:collapse;
	margin-left: 10px;
	font-size: 12px;
	text-align: center;
}
.schedule table thead{
	background-color:#e4f2ff;
	font-weight:900;
	height:40px;
	line-height:40px;
}
 td{border:1px #eaeaea solid;} 
 .teacher {
	float:right;
	width: 400px;
	height: 323px;
 }
 .teach {
	height: 80px;
	margin-top: 42px;
 }	
 .teach1 {
	margin: 0;
 }
 .teach-l {
	width: 128px;
	margin-right: 8px;
	float: left
 }
 .teach-r {
	background-color: #blue;
	height: 80px;
	width: 264px;
	float: left;
	font-size:12px;
 }
 .p2 {
	color:#fff;
	width:60px;
	background:#00ade2;
	text-align: center;
	margin:14px 0 18px 0;
 }
 .p1 {
	color:#fff;
	width:60px;
	background:#f9a948;
	text-align: center;
	margin:14px 0 18px 0;
 }
/* 内容部分 */
.title {
	margin-top: 45px;
	height: 55px;
	text-align: center;
}
.title  h2 {
	color: #007ed3;
}
.title span {
	color: #b2cde8;
	letter-spacing: 1px;
}
.dynamic { 
	height:241px;
	background-color: #fff;
	padding: 60px 0 40px 0;
}
.dynamic-l {
	margin-left: 55px;
	float: left;
	margin-right: 130px;
}
.dynamic-r {
	padding-top:5px;
	height: 241px;
	width: 500px;
	float: left;
}
.tt {
	margin-bottom: 35px;
}
.t1 {
	color: #00abe5;
}	
.t2 {
	color: #ffc146;
}
.t3 {
	color: #7e65b6;
}
.dynamic-r ul li {
	color: #00abe5;
}
.box {
	width: 112px;
	height: 32px;
	line-height: 32px;
	text-align: center;
	font-size: 12px;
	border-radius: 5px 5px 5px 5px;
	margin-top: 42px;
}
.box a {
	color: #fff;
}
.box1 {
	background-color: #00abe5;
}	
.bg {
	background-color: #f4f4f4;
	width: 100%;
	height: 365px;
}
.idea {
	height: 265px;
	padding: 50px 0;
}
.idea-l {
	padding-top:5px;
	height: 241px;
	width: 500px;
	float: left;
	margin-left: 100px;
}
.idea-l ul li {
	color: #ffc146;
}
.box2 {
	background-color: #ffc146;
}
.idea-r {
	float: left;
}
.culture{
	height: 280px;
	padding: 36px 0 40px 0;
}
.culture-r {
	padding-top:5px;
	height: 280px;
	width: 500px;
	float: left;
}
.culture-r ul li {
	color: #7e65b6;
}
.box3 {
	background-color: #7e65b6;
}
/* 底部部分 */
.footer {
	height: 91px;
	width: 100%;
}
.footer-t {
	height: 27px;
	background-color: #b3bdc6;
	text-align: center;
}
.footer-t a {
	font-size: 14px;
	color: #494949;
}
.footer-b {
	height: 44px;
	background-color: #ececec;
	text-align: center;
	padding-top: 20px;
	color: #666;
	font-size: 12px;
}